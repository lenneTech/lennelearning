{
  "name": "academy",
  "version": "1.6.1",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "homepage": "https://github.com/lenneTech/academy",
  "license": "MIT",
  "scripts": {
    "ng": "npx ng",
    "check": "npm run format:fix && npm run lint",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:fixAll": "prettier --write src",
    "start": "npx ng serve",
    "build": "npx ng build",
    "test": "npx ng test",
    "reinit": "rm -rf node_modules && rm package-lock.json && npm cache clean --force && npm i",
    "lint": "npx ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "npx ng run academy:serve-ssr",
    "serve:ssr": "npx node dist/academy/server/main.js",
    "build:ssr": "npx ng build --configuration production && npx ng run academy:server:production",
    "build:test:ssr": "npx ng build --configuration test && npx ng run academy:server:test",
    "build:develop:ssr": "npx ng build --configuration develop && npx ng run academy:server:develop",
    "prerender": "npx ng run academy:prerender",
    "prepare": "husky install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "release": "standard-version && git push --follow-tags origin develop",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin develop",
    "release:major": "standard-version --release-as major && git push --follow-tags origin develop"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lenneTech/academy"
  },
  "bugs": {
    "url": "https://github.com/lenneTech/academy/issues"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "dependencies": {
    "@angular/animations": "13.3.1",
    "@angular/cdk": "13.3.2",
    "@angular/common": "13.3.1",
    "@angular/compiler": "13.3.1",
    "@angular/core": "13.3.1",
    "@angular/forms": "13.3.1",
    "@angular/platform-browser": "13.3.1",
    "@angular/platform-browser-dynamic": "13.3.1",
    "@angular/platform-server": "13.3.1",
    "@angular/router": "13.3.1",
    "@lenne.tech/ng-base": "13.9.3",
    "@nebular/eva-icons": "9.0.1",
    "@nebular/theme": "9.0.1",
    "@nguniversal/express-engine": "13.1.0",
    "bootstrap": "5.1.3",
    "bootstrap-icons": "1.8.1",
    "cache-manager": "3.6.0",
    "eva-icons": "1.1.3",
    "express": "4.17.3",
    "lottie-web": "5.9.1",
    "ngx-highlightjs": "6.1.2",
    "ngx-lottie": "8.0.1",
    "ngx-swiper-wrapper": "10.0.0",
    "nodemailer": "6.7.3",
    "rxjs": "7.5.5",
    "sinsunsan-ngx-matomo": "0.1.5",
    "standard-version": "9.3.2",
    "swiper": "8.0.7",
    "tslib": "2.3.1",
    "zone.js": "0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.1",
    "@angular-eslint/builder": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular-eslint/eslint-plugin-template": "13.2.0",
    "@angular-eslint/schematics": "13.2.0",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.1",
    "@angular/language-service": "13.3.1",
    "@cypress/schematic": "1.6.0",
    "@lenne.tech/npm-package-helper": "0.0.10",
    "@nguniversal/builders": "13.1.0",
    "@schematics/angular": "13.3.1",
    "@types/express": "4.17.13",
    "@types/jasmine": "4.0.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "cypress": "9.5.3",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "husky": "7.0.4",
    "jasmine-core": "4.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
